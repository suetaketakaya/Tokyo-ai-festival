{
  "expo": {
    "name": "ClaudeOps Remote",
    "slug": "remote-claude-app",
    "version": "3.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.remoteclaude.app",
      "icon": "./assets/icon.png",
      "deploymentTarget": "18.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow ClaudeOps Remote to access your camera to scan QR codes.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "package": "com.remoteclaude.app",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow RemoteClaude to access your camera to scan QR codes."
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow RemoteClaude to access your camera to scan QR codes."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "401cce87-a023-4c11-b36d-f9bd2a3f5834"
      }
    },
    "owner": "takechan"
  }
}
