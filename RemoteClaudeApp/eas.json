{
  "cli": {
    "version": ">= 3.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "default"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "default",
        "simulator": true
      }
    },
    "production": {
      "ios": {
        "resourceClass": "default",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}